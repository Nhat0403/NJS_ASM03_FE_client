ify from 'alertifyjs';\nimport { addCart } from '../Redux/Action/ActionCart';\nimport CartAPI from '../API/CartAPI';\nimport queryString from 'query-string';\nimport CommentAPI from '../API/CommentAPI';\nimport convertMoney from '../convertMoney';\n\nfunction Detail(props) {\n\tconst [detail, setDetail] = useState({});\n\n\tconst dispatch = useDispatch();\n\n\t//id params cho tng sn phm\n\tconst { id } = useParams();\n\n\t//id_user c ly t redux\n\tconst id_user = useSelector((state) => state.Cart.id_user);\n\n\tconsole.log(id_user);\n\n\t//listCart c ly t redux\n\tconst listCart = useSelector((state) => state.Cart.listCart);\n\n\tconst [product, setProduct] = useState([]);\n\n\tconst [star, setStar] = useState(1);\n\n\tconst [comment, setComment] = useState('');\n\n\t// id_user  ng nhp\n\tconst idUser = useSelector((state) => state.Session.idUser);\n\n\t// Listcomment\n\tconst [list_comment, set_list_comment] = useState([]);\n\n\t// state ny dng  load li comment khi user gi comment ln\n\tconst [load_comment, set_load_comment] = useState(false);\n\n\t// Hm ny dng  ly d liu comment\n\t// Hm ny s chy li ph thuc vo id Param\n\tuseEffect(() => {\n\t\tconst fetchData = async () => {\n\t\t\tconst params = {\n\t\t\t\tidProduct: id,\n\t\t\t};\n\n\t\t\tconst query = '?' + queryString.stringify(params);\n\n\t\t\tconst response = await CommentAPI.getCommentProduct(query);\n\t\t\tconsole.log(response);\n\n\t\t\tset_list_comment(response);\n\t\t};\n\n\t\tfetchData();\n\t}, [id]);\n\n\t// Hm thay i sao nh gi\n\tconst onChangeStar = (e) => {\n\t\tsetStar(e.target.value);\n\t};\n\n\t// Hm thay i comment\n\tconst onChangeComment = (e) => {\n\t\tsetComment(e.target.value);\n\t};\n\n\t// Hm ny dng  bnh lun\n\tconst handlerComment = () => {\n\t\tif (idUser === '') {\n\t\t\talertify.set('notifier', 'position', 'bottom-left');\n\t\t\talertify.error('Vui Lng Kim Tra ng Nhp!');\n\t\t\treturn;\n\t\t}\n\n\t\tconst fetchSendComment = async () => {\n\t\t\tconst params = {\n\t\t\t\tidProduct: id